import java.lang.*;

class Variables
{
     public static void main(String arg[])
      {
         byte b=127;
         // here we can not store the value greater than 127 because it is the maximum value of byte
         short s = 333;
         int i = 100;
         
         float f = 23.5f;
         char c = 'A';
         double d = 23.342d;

         System.out.println(b);
         System.out.println(s);
         System.out.println(i);
         System.out.println(f);
         System.out.println(c);
         System.out.println(d);
       }
}
